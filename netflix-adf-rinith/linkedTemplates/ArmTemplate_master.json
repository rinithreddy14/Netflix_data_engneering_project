{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "netflix-adf-rinith"
		},
		"storage_loc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storage_loc'"
		},
		"github_loc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/rinithreddy14/Netflix_data_engneering_project/main/Raw_lookup_files/"
		},
		"storage_loc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://netflixmainstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "netflix-adf-rinith_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storage_loc_accountKey": {
						"value": "[parameters('storage_loc_accountKey')]"
					},
					"github_loc_properties_typeProperties_url": {
						"value": "[parameters('github_loc_properties_typeProperties_url')]"
					},
					"storage_loc_properties_typeProperties_url": {
						"value": "[parameters('storage_loc_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}